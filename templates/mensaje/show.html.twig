<!DOCTYPE html>

<title>Mensaje</title>

{% block body %}
    <h1>Mensaje</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ mensaje.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ mensaje.nombre }}</td>
            </tr>
            <tr>
                <th>Apellido</th>
                <td>{{ mensaje.apellido }}</td>
            </tr>
            <tr>
                <th>Telefono</th>
                <td>{{ mensaje.telefono }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ mensaje.email }}</td>
            </tr>
            <tr>
                <th>Mensaje</th>
                <td>{{ mensaje.mensaje }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('mensaje_index') }}">back to list</a>

    <a href="{{ path('mensaje_edit', {'id': mensaje.id}) }}">edit</a>

    {{ include('mensaje/_delete_form.html.twig') }}
{% endblock %}
