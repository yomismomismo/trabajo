<!-- Thank you section -->
<section id="thankYou" class="container" style="display:none;">
    <h1>Hey, thanks!</h1>
    <p>
      Thanks for submitting your super personal information. Again, we probably won't sell it for that sweet bank — relax. Want to submit again? Maybe a friend's info? Click below!
    </p>
    <a href="#" id="restartForm">Restart form</a>
  </section>
  
  <!-- Contact form section -->
  <section id="contactForm" class="container" itemscope itemtype="http://schema.org/ContactPage">
    <h1 itemprop="mainContentOfPage">Become a member</h1>
    <p itemprop="description">
      You want to become a member, right? Of course you do – everyone does. Just fill out the simple little form below and submit your very personal information to us. We promise we won't sell it or anything... probably.
    </p>
    
    <!--  START FORM  -->
    <form id="submissionForm" class="form" action="/" method="post" autocomplete="on">
      <legend for="submissionForm">Contáctanos</legend>
  
      <!--   FORM => First/last name   -->
      <div id="formNames" class="form-group width-1-1 child-width-1-2">
        <div>
          <label class="form-label" for="firstName">
            Nombre<abbr title="required">*</abbr>
          </label>
          <input id="firstName" class="form-element" name="given-name" type="text" autocomplete="given-name" placeholder="John? Jacob?" tabindex="1" required aria-required="true" autofocus />
        </div>
        <div>
          <label class="form-label" for="lastName">
            Apellidos<abbr title="required">*</abbr>
          </label>
          <input id="lastName" class="form-element" name="family-name" type="text" autocomplete="family-name" placeholder="Jingleheimer Schmidt?" tabindex="2" required aria-required="true" />
        </div>
      </div>
  
      <!--   FORM => Email, phone   -->
      <div id="formEmailPhone" class="form-group width-1-1 child-width-3-4">
        <div>
          <label class="form-label" for="emailAddress">
            Email<abbr title="required">*</abbr>
          </label>
          <input id="emailAddress" class="form-element" name="emailAddress" type="email" autocomplete="on" placeholder="yourname@gmail(probably).com" tabindex="3" required aria-required="true" />
        </div>
        <div>
          <label class="form-label" for="phoneNumber">
            Número de télefono<abbr title="required">*</abbr>
          </label>
          <input id="phoneNumber" class="form-element" name="phoneNumber" type="tel" autocomplete="on" placeholder="000-00-00-00" tabindex="4" required aria-required="true" />
        </div>
      </div>
  
      <!--   FORM => Address line 1 & 2   -->
          <label class="form-label" for="mensaje">Mensaje<abbr title="required">*</abbr></label>
          <div id="divTextarea">
